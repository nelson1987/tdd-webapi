{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "RabbitMQConnection": "amqp://guest:guest@localhost:5672"
  },
  "Aspire": {
    "RabbitMQ": {
      "Client": {
        "DisableHealthChecks": true
      }
    },
    "SqlServer": {
      "SqlClient": {
        "ConnectionString": "YOUR_CONNECTIONSTRING",
        "DbContextPooling": true,
        "DisableHealthChecks": true,
        "DisableTracing": true,
        "DisableMetrics": false
      }
    }
  },
  "AllowedHosts": "*"
}
